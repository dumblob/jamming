{
	"name": "jamming",
	"version": "0.1.0",
	"description": "Library to handle low latency audio conferencing.",
	"main": "lib/index.js",
	"private": true,
	"scripts": {
		"build": "yarn run build:lib && yarn run build:webpack",
		"build:lib": "babel src/ -d lib/",
